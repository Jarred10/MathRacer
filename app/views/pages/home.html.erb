<h1>Math Racer</h1>

<% form_for :user, @user do |f| %>
	<p> Username: <%= f.text_field :username %> </p>
	<p> Email: <%= f.text_field :email %> </p>
	<p> Password: <%= f.password_field :password %> </p>
	<%= f.submit :Signup %>
<% end %>

<%= link_to 'Users', users_path %>
<%= link_to 'Games', games_path %>
